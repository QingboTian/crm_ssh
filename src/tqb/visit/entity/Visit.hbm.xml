<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tqb.visit.entity.Visit" table="t_visit">
		<id name="vid" column="vid">
			<generator class="native"></generator>
		</id>
		<property name="time" column="time"></property>
		<property name="content" column="content"></property>
		
		<many-to-one name="customer" class="tqb.customer.entity.Customer" column="cid"></many-to-one>
		<many-to-one name="user" class="tqb.user.entity.User" column="uid"></many-to-one>
	</class>
</hibernate-mapping>